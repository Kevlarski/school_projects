<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" minHeight="300.0" minWidth="252.0" prefHeight="300.0" prefWidth="252.0" style="-fx-border-radius: 25px;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="proj3.MoviesFXMLController">
   <children>
      <Button fx:id="loadBtn" layoutX="183.0" layoutY="265.0" mnemonicParsing="false" onAction="#load" text="Load" />
      <Button fx:id="addBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#add" text="Add" />
      <Button fx:id="saveBtn" layoutX="14.0" layoutY="265.0" mnemonicParsing="false" onAction="#save" text="Save" />
      <Button fx:id="removeBtn" layoutX="89.0" layoutY="265.0" mnemonicParsing="false" onAction="#remove" text="Remove" />
      <ListView fx:id="list" layoutX="14.0" layoutY="50.0" prefHeight="200.0" prefWidth="221.0" />
      <TextField fx:id="addInput" layoutX="67.0" layoutY="14.0" prefHeight="24.0" prefWidth="166.0" promptText="Enter movie here!" />
   </children>
</AnchorPane>
